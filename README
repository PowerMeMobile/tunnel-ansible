vagrant up

cd ansible
ansible all -i hosts -m ping
server | success >> {
    "changed": false,
    "ping": "pong"
}

client | success >> {
    "changed": false,
    "ping": "pong"
}

ansible-playbook setup.yml -i hosts

vagrant ssh server

